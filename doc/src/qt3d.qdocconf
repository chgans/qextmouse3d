include(qt-cpp-ignore.qdocconf)

project                 = Qt/3D
description             = Qt/3D Documentation
url                     = http://doc.trolltech.com/qt/qt3d

language                = Cpp
imagedirs               = $TOPSRC/doc/src/images
indexes                 = 
headerdirs              = $TOPSRC/threed $TOPSRC/demos/qml3d $TOPSRC/opencl
headers.fileextensions  = "*.h"
sourcedirs              = $TOPSRC/threed $TOPSRC/doc/src $TOPSRC/demos/qml3d $TOPSRC/opencl
sources.fileextensions  = "*.qdoc *.cpp"
exampledirs             = $TOPSRC/examples $TOPSRC/demos
examples.fileextensions = "*.cpp *.h *.js *.xq *.svg *.xml *.ui *.qrc *.txt"
outputdir               = $DESTDIR
base                    = file:$DESTDIR
defines                 = 
